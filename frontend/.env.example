# RAG Platform Frontend Environment Variables
# Copy this file to .env and configure as needed

# Backend API URL
# For local development, use: http://localhost:8001
# For production, this will be set by the deployment environment
REACT_APP_BACKEND_URL=http://localhost:8001

# WebSocket configuration for development server
# This is required for hot-reload to work properly
WDS_SOCKET_PORT=443
